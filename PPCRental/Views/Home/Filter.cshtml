@model IEnumerable<PPCRental.Models.PROPERTY>

<div class="parallax-selector"></div>
<section>
    <div class="section-header text-center">
        <h1 class="heading is-section">Danh sách dự án</h1>
    </div>
    <div class="row-md4-content">
        @{int count = 0; }
        @foreach (var item in Model)
        {
            <div class="col-md-4">
                <a href="">
                    <div class="row outside-img">
                        <img class="img-responsive" src="~/Images/behind-content.jpg" alt="a">
                        <img class="img-responsive" src="~/Images/behind-content.jpg" alt="a">
                    </div>
                    <div class="list-img-banner">Mới</div>
                    <div class="row content-project">
                        <div class="col-md-7 col-left">
                            <h3 class="text-left">@item.PropertyName<br>Quận 1</h3>
                        </div>
                        <div class="col-md-5 col-right">
                            <p class="text-right">@item.Price</p>
                        </div>
                    </div>
                </a>
            </div>
        }
