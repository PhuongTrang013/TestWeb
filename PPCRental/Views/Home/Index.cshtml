                    <h1 class="heading is-section">Danh sách dự án</h1>
                </div>
                @*<div class="container Featured-Sales">*@
                <div class="row-md4-content">
                    @{ int count = 0; }
                    @{ foreach (var item in Model)
                        {
                            <div class="col-md-4">
                                <a href='/ViewDetailOfProjectUser/ViewDetailOfProject/@item.ID'>
                                    <div class="row outside-img">
                                        <img class="img-responsive" src="~/Images/@item.Images" alt="">
                                        <img class="img-responsive img-last" src="~/Images/nha1.2.jpg" alt="">
                                    </div>
                                    <div class="list-img-banner">Mới</div>
                                    <div class="content-project">
                                        <h3>Căn hộ Phú Mỹ Hưng <br>7 Phú Mỹ Hưng</h3>
                                        <p>500.000.000 <br>Lê Vân Nguyệt</p>
                                    </div>
                                </a>
                            </div>
                            count++;
                            if (count > 5)
                            {
                                break;
                            }
                        }
                    }
                    <a class="button-all-sale" href="">Xem Thêm</a>
                </div>
            </section>
        </div>
